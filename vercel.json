{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["db.json"]
			}
		},
		{ "src": "*.html", "use": "@vercel/static" },
		{ "src": "*.js", "use": "@vercel/vite" }
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.html"
		}
	]
}
